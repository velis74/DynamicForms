{
  "name": "dynamicforms",
  "version": "1.0.0",
  "description": "Data entry boilerplate components and a RESTful API consumer",
  "author": "Jure Erzno≈ænik",
  "scripts": {
    "serve": "vite",
    "build": "vite build",
    "test": "vitest run --coverage",
    "lint": "eslint vue"
  },
  "main": "./vue/dynamicforms.js",
  "keywords": [
    "vue",
    "dynamicforms",
    "velis"
  ],
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git@github.com:velis74/DynamicForms.git"
  },
  "issues": "https://github.com/velis74/DynamicForms/issues",
  "dependencies": {
    "@vuepic/vue-datepicker": "^3.5.3",
    "axios": "^1.2.0",
    "bootstrap": "<5.0.0",
    "bootstrap-vue": "^2.21.2",
    "enum": "^3.0.4",
    "lodash": "^4.17.15",
    "luxon": "^1.28.1",
    "resize-observer-polyfill": "^1.5.1",
    "vue": "^3.2.45",
    "vue-ionicon": "^2.1.0",
    "vue-multiselect": "^3.0.0-alpha.2",
    "vue-observe-visibility": "^1.0.0",
    "vue-router": "^4.1.6",
    "vue3-datetime": "^2.0.0-beta.3",
    "vuetify": "^3.0.1",
    "weekstart": "^1.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.2.0",
    "@types/jest": "^29.2.5",
    "@types/lodash": "^4.14.191",
    "@types/luxon": "^1.27.1",
    "@vitest/coverage-c8": "^0.25.2",
    "@vue/test-utils": "^2.2.4",
    "@vue/tsconfig": "^0.1.3",
    "cache-loader": "^4.1.0",
    "eslint-config-velis": "^1.1.13",
    "flush-promises": "^1.0.2",
    "http-proxy-middleware": "^2.0.6",
    "jest": "^29.3.1",
    "jsdom": "^20.0.3",
    "np": "^7.6.0",
    "sass": "~1.32.0",
    "typescript": "^4.9.3",
    "vite": "^3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vuetify": "^1.0.0",
    "vitest": "^0.25.2",
    "vue-tsc": "^1.0.9",
    "webpack": "^4.46.0"
  },
  "vue": {
    "outputDir": "./dynamicforms/static/vue/",
    "css": {
      "extract": {
        "ignoreOrder": true,
        "filename": "css/dynamicforms.[hash:8].css",
        "chunkFilename": "css/dynamicforms.[name].css"
      }
    },
    "transpileDependencies": [
      "vuetify"
    ]
  },
  "babel": {
    "presets": [
      "@vue/cli-plugin-babel/preset"
    ]
  },
  "eslintConfig": {
    "extends": [
      "velis"
    ],
    "rules": {
      "vuejs-accessibility/click-events-have-key-events": "off",
      "vuejs-accessibility/form-control-has-label": "off",
      "vuejs-accessibility/label-has-for": "off"
    },
    "ignorePatterns": [
      "dist/*",
      "coverage/*",
      "node_modules/*",
      "vite.config.ts"
    ]
  },
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest",
    "roots": [
      "./vue/"
    ],
    "testMatch": [
      "**/*.spec.[jt]s?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(vue-ionicon))"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,ts,vue}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!index.*"
    ]
  }
}
