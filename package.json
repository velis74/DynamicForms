{
  "name": "dynamicforms",
  "version": "1.0.0",
  "description": "Data entry boilerplate components and a RESTful API consumer",
  "author": "Jure Erzno≈ænik",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "main": "./vue/dynamicforms.js",
  "keywords": [
    "vue",
    "dynamicforms",
    "velis"
  ],
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git@github.com:velis74/DynamicForms.git"
  },
  "issues": "https://github.com/velis74/DynamicForms/issues",
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^33.0.0",
    "@ckeditor/ckeditor5-vue2": "^1.0.5",
    "axios": "<1.0.0",
    "bootstrap": "<5.0.0",
    "bootstrap-vue": "^2.21.2",
    "dompurify": ">=2.3.4",
    "enum": "^3.0.4",
    "lodash": "^4.17.15",
    "luxon": "^1.28.0",
    "resize-observer-polyfill": "^1.5.1",
    "vue": "^2.6.14",
    "vue-datetime": "^1.0.0-beta.14",
    "vue-ionicon": "^1.1.1",
    "vue-multiselect": "^2.1.6",
    "vue-observe-visibility": "^1.0.0",
    "vue-router": "^3.5.2",
    "vuetify": "^2.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-unit-jest": "^4.5.0",
    "@vue/cli-service": "^4.5.15",
    "@vue/eslint-config-airbnb": ">=5.3.0 <6.0.0",
    "@vue/test-utils": "^1.3.0",
    "babel-eslint": ">=10.1.0",
    "cache-loader": "^4.1.0",
    "eslint": "^6.8.0",
    "eslint-config-velis": "^1.0.3",
    "eslint-plugin-es": ">=4.1.0",
    "eslint-plugin-vue": ">=8.3.0",
    "flush-promises": "^1.0.2",
    "np": "^7.6.0",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "vue-cli-plugin-vuetify": "~2.4.5",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.0",
    "webpack": "^4.46.0"
  },
  "vue": {
    "outputDir": "./dynamicforms/static/vue/",
    "css": {
      "extract": {
        "ignoreOrder": true,
        "filename": "css/dynamicforms.[hash:8].css",
        "chunkFilename": "css/dynamicforms.[name].css"
      }
    },
    "configureWebpack": {
      "entry": {
        "app": "./vue/main.js"
      },
      "resolve": {
        "extensions": [
          ".js",
          ".vue",
          ".json"
        ]
      },
      "devServer": {
        "proxy": {
          ".*": {
            "target": "http://localhost:8000",
            "secure": false
          }
        }
      },
      "output": {
        "filename": "js/dynamicforms.[hash:8].js",
        "chunkFilename": "js/dynamicforms.[name].js"
      },
      "optimization": {
        "minimize": false,
        "splitChunks": false
      }
    },
    "transpileDependencies": [
      "vuetify"
    ]
  },
  "babel": {
    "presets": [
      "@vue/cli-plugin-babel/preset"
    ]
  },
  "eslintConfig": {
    "extends": [
      "velis"
    ]
  },
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest",
    "roots": [
      "./vue/"
    ],
    "testMatch": [
      "**/*.spec.[jt]s?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(vue-ionicon))"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!index.*"
    ]
  }
}
