{
  "name": "@velis/dynamicforms",
  "private": false,
  "version": "0.70.47",
  "description": "Data entry boilerplate components and a RESTful API consumer",
  "author": "Jure Erzno≈ænik",
  "files": [
    "dist/*"
  ],
  "main": "dist/dynamicforms.umd.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/dynamicforms.umd.js",
      "import": "./dist/dynamicforms.mjs"
    },
    "./styles.css": "./dist/style.css"
  },
  "scripts": {
    "build-no-tsc": "vite build",
    "build": "vite build && vue-tsc --emitDeclarationOnly -p tsconfig.build.json",
    "test": "vitest run --coverage",
    "lint": "eslint src && vue-tsc --noEmit",
    "watch": "vite build --watch"
  },
  "keywords": [
    "vue",
    "dynamicforms",
    "velis"
  ],
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git@github.com:velis74/DynamicForms.git"
  },
  "issues": "https://github.com/velis74/DynamicForms/issues",
  "peerDependencies": {
    "axios": "^1.2.0",
    "bootstrap": "<5.0.0",
    "bootstrap-vue": "^2.21.2",
    "lodash": "^4.17.15",
    "vue": "^3.2.45",
    "vue-ionicon": "^2.1.0",
    "vue-router": "^4.1.6",
    "vuetify": "^3.0.1"
  },
  "dependencies": {
    "luxon": "^1.28.1",
    "resize-observer-polyfill": "^1.5.1",
    "vue-datetime3": "^1.0.2",
    "vue-multiselect": "^3.0.0-beta.1",
    "vue-observe-visibility": "^1.0.0",
    "weekstart": "^1.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/lodash": "^4.14.191",
    "@types/luxon": "^1.27.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.28.3",
    "@vue/test-utils": "^2.2.4",
    "@vue/tsconfig": "^0.1.3",
    "cache-loader": "^4.1.0",
    "eslint-config-velis": "^1.1.13",
    "flush-promises": "^1.0.2",
    "jsdom": "^21",
    "np": "^7.7.0",
    "sass": "^1",
    "typescript": "^4.9.3",
    "vite": "^4",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vuetify": "^1.0.0",
    "vitest": "^0",
    "vue-tsc": "^1.0.9"
  },
  "eslintConfig": {
    "extends": [
      "velis"
    ],
    "rules": {
      "vuejs-accessibility/click-events-have-key-events": "off",
      "vuejs-accessibility/form-control-has-label": "off",
      "vuejs-accessibility/label-has-for": "off"
    },
    "ignorePatterns": [
      "dist/*",
      "coverage/*",
      "node_modules/*"
    ]
  }
}
