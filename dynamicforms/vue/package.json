{
  "name": "dynamicforms",
  "version": "0.1.115",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "29.0.0",
    "@ckeditor/ckeditor5-vue2": "1.0.5",
    "@vue/cli": "^4.5.13",
    "axios": "^0.21.1",
    "bootstrap": "^4.0.0-beta",
    "core-js": "^3.6.5",
    "jquery": "^3.6.0",
    "lodash": "^4.17.15",
    "popper.js": "^1.16.1",
    "select2": "^4.1.0-rc.0",
    "vue": "^2.6.11",
    "vue-datetime": "^1.0.0-beta.14",
    "vue-multiselect": "^2.1.6",
    "vue-observe-visibility": "^1.0.0",
    "vue-router": "^3.5.2",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "^1.2.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-vue": "^7.13.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "plugins": [
      "es"
    ],
    "extends": [
      "plugin:vue/essential",
      "plugin:vue/strongly-recommended",
      "plugin:vue/recommended",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "max-len": ["error", { "code": 120 }],
      "no-console": 0,
      "no-plusplus": "off",
      "sort-imports": "off",
      "import/order": [
        "error",
        {
          "newlines-between": "always",
          "alphabetize": { "order": "asc", "caseInsensitive": true }
        }
      ],
      "indent": ["error", 2, { "MemberExpression": 1 }],
      "es/no-destructuring": "error",
      "no-param-reassign": ["error", { "props": false }],
      "prefer-destructuring": [
        "warn",
        {
          "VariableDeclarator": { "array": false, "object": false },
          "AssignmentExpression": { "array": false, "object": false }
        },
        { "enforceForRenamedProperties": false }
      ],
      "no-underscore-dangle": ["error", { "allowAfterThis": true }],
      "operator-linebreak": ["error", "after"],
      "linebreak-style": ["off"],
      "vue/html-closing-bracket-spacing": ["error", { "selfClosingTag": "never" }],
      "vue/no-v-html": ["off"],
      "vue/max-attributes-per-line": ["warn", { "singleline": { "max": 10 } }],
      "vue/singleline-html-element-content-newline": ["off"]
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "since 2015",
    "not dead"
  ]
}
